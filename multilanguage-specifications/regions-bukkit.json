[
  {
    "key": "regions.message.region",
    "description": "Shows a list of commands for the region system.",
    "default": [
      "Region Commands:",
      "  /region info - Shows information about the region you are standing in.",
      "  /region visualize [regionId] - Visualizes the region with the given id."
    ]
  },
  {
    "key": "regions.message.region.info.header",
    "description": "Shows the header of the region info.",
    "default": "There were {amount} regions found at {x}, {y}, {z}:",
    "placeholders": [
      {
        "key": "amount",
        "description": "The amount of regions found.",
        "example": "7"
      },
      {
        "key": "x",
        "description": "The x coordinate of the location.",
        "example": "100"
      },
      {
        "key": "y",
        "description": "The y coordinate of the location.",
        "example": "64"
      },
      {
        "key": "z",
        "description": "The z coordinate of the location.",
        "example": "100"
      }
    ]
  },
  {
    "key": "regions.message.region.info.region",
    "description": "Shows information about a region.",
    "default": "  {id} - {priority} - {parts}",
    "placeholders": [
      {
        "key": "id",
        "description": "The id of the region.",
        "example": "42w36au"
      },
      {
        "key": "priority",
        "description": "The priority of the region.",
        "example": "2"
      },
      {
        "key": "parts",
        "description": "The parts of the region.",
        "example": "4"
      }
    ]
  },
  {
    "key": "regions.message.region.info.footer",
    "description": "Shows the footer of the region info.",
    "default": "Use /region visualize [regionId] to visualize a region."
  },
  {
    "key": "regions.message.region.visualize.start",
    "description": "Shows a message when a region is visualized.",
    "default": "Visualized region {id}.",
    "placeholders": [
      {
        "key": "id",
        "description": "The id of the region.",
        "example": "42w36au"
      }
    ]
  },
  {
    "key": "regions.message.region.visualize.stop",
    "description": "Shows a message when a region is no longer visualized.",
    "default": "Stopped visualizing region {id}.",
    "placeholders": [
      {
        "key": "id",
        "description": "The id of the region.",
        "example": "42w36au"
      }
    ]
  },
  {
    "key": "regions.message.region.no-regions",
    "description": "Shows a message when no regions are found at the given location.",
    "default": "No regions found at {x}, {y}, {z}.",
    "placeholders": [
      {
        "key": "x",
        "description": "The x coordinate of the location.",
        "example": "100"
      },
      {
        "key": "y",
        "description": "The y coordinate of the location.",
        "example": "64"
      },
      {
        "key": "z",
        "description": "The z coordinate of the location.",
        "example": "100"
      }
    ]
  },
  {
    "key": "regions.message.region.no-region",
    "description": "Shows a message when no region is found with the given id.",
    "default": "No region found with id {id}.",
    "placeholders": [
      {
        "key": "id",
        "description": "The id of the region.",
        "example": "1"
      }
    ]
  }
]
